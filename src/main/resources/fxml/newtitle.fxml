<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="titlePane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NewTitleController">
   <children>
      <TextArea fx:id="textInput" layoutX="16.0" layoutY="120.0" prefHeight="103.0" prefWidth="569.0" />
      <Button fx:id="saveButton" layoutX="238.0" layoutY="265.0" mnemonicParsing="false" onAction="#saveAndCloseAction" onMouseClicked="#saveAndCloseAction" prefHeight="46.0" prefWidth="72.0" stylesheets="@../css/stylesheet.css">
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/saveIcon.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="cancelButton" cancelButton="true" layoutX="222.0" layoutY="311.0" mnemonicParsing="false" onAction="#cancelAction" stylesheets="@../css/stylesheet.css" text="Cancel">
         <graphic>
            <ImageView fitHeight="23.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/cross.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Text fx:id="messageBox" fontSmoothingType="LCD" layoutX="16.0" layoutY="98.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="569.0">
         <font>
            <Font name="PT Mono Bold" size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
